print("The Love Calculator is calculating your score...")
name1 = input() # What is your name?
name2 = input() # What is their name?
couple_names= name1+name2
lowercase=couple_names.lower()
t=lowercase.count("t")
r=lowercase.count("r")
u=lowercase.count("u")
e=lowercase.count("e")
first_digit=t+r+u+e
l=lowercase.count("l")
o=lowercase.count("o")
v=lowercase.count("v")
e=lowercase.count("e")
second_digit=l+o+v+e
two_digit=str(first_digit)+str(second_digit)
if int(two_digit)<10 or int(two_digit)>90:
  {
    print(f"Your score is {two_digit}, you go together like coke and mentos.")
  }
elif  int(two_digit)>40 and int(two_digit)<50:
  {
    print(f"Your score is {two_digit}, you are alright together.")
  }
else:
  {
    print(f"Your score is {two_digit}.")
  }
  
